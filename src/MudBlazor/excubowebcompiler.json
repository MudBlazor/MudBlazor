{
  "Minifiers": {
    "GZip": false,
    "Enabled": true,
    "Css": {
      "TermSemicolons": true,
      "OutputMode": "SingleLine",
    }
  },
  "Javascript": {
    "RenameLocals": true,
    "PreserveImportantComments": true,
    "EvalTreatment": "Ignore",
    "TermSemicolons": true,
    "OutputMode": "SingleLine",
    "IndentSize": 2
  },
  "CompilerSettings": {
    "Sass": {
      "IndentType": "Space",
      "IndentWidth": 2,
      "OutputStyle": "Nested",
      "Precision": 5,
      "RelativeUrls": true,
      "LineFeed": "Lf",
      "SourceMap": false
    }
  },
  "Output": {
    "Preserve": true,
    "Directory": "./wwwroot"
  }
}