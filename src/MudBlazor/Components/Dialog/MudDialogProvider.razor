@namespace MudBlazor
<div id="@_elementId">
    <CascadingValue Value="this" IsFixed="true">
        <CascadingValue Value="_globalDialogOptions">
            @foreach (var dialogReference in VisibleDialogs)
            {
                @dialogReference.RenderFragment
            }
        </CascadingValue>
    </CascadingValue>
</div>