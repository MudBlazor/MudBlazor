@namespace MudBlazor

<CascadingValue Value="this" IsFixed="true">
    <CascadingValue Value="_globalDialogOptions">
        @foreach (var dialogReference in DialogService.Dialogs.Where(x => !x.Result.IsCompleted))
        {
            @dialogReference.RenderFragment
        }
    </CascadingValue>
</CascadingValue>
