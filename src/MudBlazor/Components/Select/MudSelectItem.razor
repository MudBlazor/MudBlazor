@namespace MudBlazor
@typeparam T
@inherits MudBaseSelectItem

<MudListItem @attributes="UserAttributes" T="T" Value="@Value" id="@ItemId" OnClick="@OnClicked" Disabled="@Disabled" Class="@GetCssClasses()" Style="@Style">
    @if (ChildContent != null)
    {
        @ChildContent
    }
    else
    {
        @DisplayString
    }
</MudListItem>


