@namespace MudBlazor
<div id="mud-portal-container"  data-items=@(Portal.Items.Count)>
    @foreach (var  item in Portal.Items)
    {
      <div style=@AnchorStyle(item) 
           class=@AnchorClass
           @key=@item.Id 
           id="@item.Id">

          @item.Fragment

      </div>
    }
</div>