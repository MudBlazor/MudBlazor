@namespace MudBlazor.Docs.Examples


<MudPaper Width="300px">
    <MudList T="string" Clickable="true">
        <MudListSubheader>
            Nested List Items
        </MudListSubheader>
        <MudListItem T="string" Icon="@Icons.Filled.Send">
            Sent mail
        </MudListItem>
        <MudListItem T="string" Icon="@Icons.Filled.Drafts">
            Drafts
        </MudListItem>
        <MudListItem T="string" Icon="@Icons.Filled.Inbox" Text="Inbox" InitiallyExpanded="true">
            <NestedList>
                <MudListItem T="string" Icon="@Icons.Filled.StarRate">
                    Starred
                </MudListItem>
                <MudListItem T="string" Icon="@Icons.Filled.WatchLater">
                    Snoozed
                </MudListItem>
            </NestedList>
        </MudListItem>
    </MudList>
</MudPaper>