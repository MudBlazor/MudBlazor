{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
       
        {
            "cwd": "${workspaceFolder}/src/MudBlazor.UnitTests.Viewer",
            "type": "blazorwasm",
            "name": "unit test viewer",
            "request": "launch"
        },
        {
            "cwd": "${workspaceFolder}/src/MudBlazor.Docs.Client",
            "type": "blazorwasm",
            "name": "docs client",
            "request": "launch"
        },
        {
            "name": "docs server",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/MudBlazor.Docs.Server/bin/Debug/net5.0/MudBlazor.Docs.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/MudBlazor.Docs.Server",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}